<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="coupClient" class="com.neodem.coup.client.network.CoupClient">
        <property name="messageTranslator" ref="messageTranslator"/>
    </bean>

    <bean id="messageTranslator" class="com.neodem.coup.common.messaging.JsonMessageTranslator"/>
</beans>